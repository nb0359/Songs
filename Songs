public class Songs {
    private String artist, title, genre;
    private double obscene;
    private int year;

    public Songs(String title, String artist, String genre, double obscene, int year){
        this.title = title;
        this.artist=artist;
        this.genre = genre;
        this.obscene = obscene;
        this.year = year;
    }

    public String getTitle() {
        return title;
    }

    public String getArtist() {
        return artist;
    }

    public String getGenre() {
        return genre;
    }

    public double getObscene() {
        return obscene;
    }

    public int getYear() {
        return year;
    }




}
